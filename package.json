    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "concurrently": "^8.0.1"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "keywords": ["investissement", "gazoduc", "finance"],
  "author": "Votre Nom",
  "license": "ISC"
}
